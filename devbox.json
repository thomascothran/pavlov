{
  "packages": [
    "jdk@21",
    "nodejs@23.2.0"
  ],
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!' > /dev/null"
    ],
    "scripts": {
      "cljs-test-watch": [
        "node --watch out/node-tests.js"
      ],
      "deploy": [
        "clj -X:test",
        "clj -T:build ci",
        "env $(cat .secrets | xargs) clj -T:build deploy"
      ],
      "squint-watch": [
        "npx squint watch"
      ],
      "test": [
        "clj -X:test"
      ],
      "test-watch": [
        "clj -X:test :watch? true"
      ]
    }
  },
  "nixpkgs": {
    "commit": "f80ac848e3d6f0c12c52758c0f25c10c97ca3b62"
  }
}